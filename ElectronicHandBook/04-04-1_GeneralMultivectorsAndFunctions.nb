Cell["Factorization of blade", "Section",
 CounterAssignments->{{"Section", 2}, {"Subsubsection", 0}, {"Item", 27}, {
   "Subitem", 0}, {"Subsubitem", 0}, {"ItemNumbered", 0}, {
