                    StyleBox["k", "TI"]], 
                    SubsuperscriptBox[
                    StyleBox["C", "TI"], 
                    RowBox[{
                    StyleBox["m", "TI"], "-", 
                    StyleBox["i", "TI"]}], 
                    RowBox[{
                    StyleBox["n", "TI"], "-", 
                    StyleBox["k", "TI"]}]]}]}], ")"}], 
               SubscriptBox[
                RowBox[{"\[LeftAngleBracket]", 
                  StyleBox["U", "TI"], "\[RightAngleBracket]"}], 
                StyleBox["k", "TI"]]}]}, {"\[Null]", 
