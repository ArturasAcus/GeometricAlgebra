    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "b446c8b0-7c95-43ad-93cc-a98484834c59"],
 " and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SuperscriptBox[
          StyleBox["A", "TI"], "2"], "\[LongEqual]", "3"}], TraditionalForm], 
     "errors" -> {}, "input" -> "\\m{A}^2=3", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "60969b83-95f2-4dbe-a81f-a13b8d3156fa"],
 ". The MV determinant  can be computed with ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["D", "TI"], "\[LongEqual]", 
         FractionBox["1", "3"], 
         StyleBox["H", "TI"], 
         RowBox[{"(", 
           RowBox[{
             SubscriptBox[
              RowBox[{"(", 
                RowBox[{
                  StyleBox["H", "TI"], 
                  SubscriptBox[
                   RowBox[{"(", 
                    RowBox[{
                    StyleBox["H", "TI"], 
                    SubscriptBox[
                    StyleBox["H", "TI"], 
                    RowBox[{
                    OverscriptBox["1", "_"], ",", 
                    OverscriptBox["5", "_"]}]]}], ")"}], 
                   OverscriptBox["4", "_"]]}], ")"}], 
              RowBox[{
                OverscriptBox["1", "_"], ",", 
                OverscriptBox["5", "_"]}]], "+", "2", 
             SubscriptBox[
              RowBox[{"(", 
                RowBox[{
                  SubscriptBox[
                   StyleBox["H", "TI"], 
                   RowBox[{
                    OverscriptBox["4", "_"], ",", 
                    OverscriptBox["5", "_"]}]], 
                  SubscriptBox[
                   RowBox[{"(", 
                    RowBox[{
                    SubscriptBox[
                    StyleBox["H", "TI"], 
                    RowBox[{
                    OverscriptBox["4", "_"], ",", 
                    OverscriptBox["5", "_"]}]], 
                    SubscriptBox[
                    StyleBox["H", "TI"], 
                    RowBox[{
                    OverscriptBox["1", "_"], ",", 
                    OverscriptBox["4", "_"]}]]}], ")"}], 
                   OverscriptBox["4", "_"]]}], ")"}], 
              RowBox[{
                OverscriptBox["1", "_"], ",", 
                OverscriptBox["4", "_"]}]]}], ")"}]}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "D=\\frac{1}{3} \\m{H} \\Bigl(\\bigl(\\m{H} (\\m{H}\n\
\\m{H}_{\\bar{1},\\bar{5}})_{\\bar{4}}\\bigr)_{\\bar{1},\\bar{5}} + 2\n\
\\bigl(\\m{H}_{\\bar{4},\\bar{5}} (\\m{H}_{\\bar{4},\\bar{5}}\n\
\\m{H}_{\\bar{1},\\bar{4}})_{\\bar{4}}\\bigr)_{\\bar{1},\\bar{4}}\\Bigr)", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "5d29bfb0-2d80-489a-8b76-a054fa2301c7"],
 ", where  ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
