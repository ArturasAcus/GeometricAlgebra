               SubsuperscriptBox[
                StyleBox["e", FontSlant -> "Plain"], 
                StyleBox["A", "TI"], "-1"], 
               StyleBox["U", "TI"], 
               SubscriptBox[
                StyleBox["e", FontSlant -> "Plain"], 
                StyleBox["A", "TI"]]}], 
             RowBox[{"\[LongEqual]", 
               UnderoverscriptBox["\[Sum]", 
                RowBox[{
                  StyleBox["k", "TI"], "\[LongEqual]", "0"}], 
                StyleBox["n", "TI"], LimitsPositioning -> True], 
               SuperscriptBox[
                RowBox[{"(", "-1", ")"}], 
                RowBox[{
                  StyleBox["k", "TI"], 
                  StyleBox["m", "TI"]}]], 
               RowBox[{"(", 
