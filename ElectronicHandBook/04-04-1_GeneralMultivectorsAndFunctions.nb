               SubsuperscriptBox[
                StyleBox["B", "TI"], 
                StyleBox["r", "TI"], "\[Prime]"], "\[LongEqual]", "-", 
               FractionBox["1", "5"], 
               SubscriptBox[
                StyleBox["e", FontSlant -> "Plain"], "2"], "+", 
               SubscriptBox[
                StyleBox["e", FontSlant -> "Plain"], "3"], "-", 
