          StyleBox["e", FontSlant -> "Plain"], "13"]}], TraditionalForm], 
     "errors" -> {}, "input" -> "F_r^{-1}=F_r=\\e{13}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "a5aa36ff-20c1-4308-8df1-a9884b0e8a0a"],
 ". Then, \[LineSeparator]\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       Cell[
        BoxData[
         FormBox[
          GridBox[{{"\[Null]", 
             RowBox[{
               SubscriptBox[
                StyleBox["b", "TI"], "1"], "\[LongEqual]", 
               RowBox[{"(", 
                 SubscriptBox[
                  StyleBox["e", FontSlant -> "Plain"], "1"], 
                 "\[RightFloor]"}], 
               SubsuperscriptBox[
                StyleBox["F", "TI"], 
                StyleBox["r", "TI"], "-1"], ")", "\[RightFloor]", 
               SubsuperscriptBox[
                StyleBox["B", "TI"], 
                StyleBox["r", "TI"], "\[Prime]"], "\[LongEqual]", 
               RowBox[{"(", 
                 SubscriptBox[
                  StyleBox["e", FontSlant -> "Plain"], "1"], 
                 "\[RightFloor]"}], 
               SubscriptBox[
                StyleBox["e", FontSlant -> "Plain"], "13"], ")", 
               "\[RightFloor]", 
               SubsuperscriptBox[
                StyleBox["B", "TI"], 
                StyleBox["r", "TI"], "\[Prime]"], "\[LongEqual]", 
               SubscriptBox[
                StyleBox["e", FontSlant -> "Plain"], "1"], "-", 
