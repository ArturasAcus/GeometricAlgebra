                   UnderoverscriptBox["\[Sum]", 
                    RowBox[{
                    StyleBox["i", "TI"], "\[LongEqual]", "0"}], 
                    StyleBox["m", "TI"], LimitsPositioning -> True], 
                   SuperscriptBox[
                    RowBox[{"(", "-1", ")"}], 
                    StyleBox["i", "TI"]], 
                   RowBox[{
                    SubsuperscriptBox[
                    StyleBox["C", "TI"], 
