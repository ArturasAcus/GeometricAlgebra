 RowBox[{
  RowBox[{"outerProductViaDeterminant", "[", "vecOutProd_OuterProduct", "]"}],
   ":=", 
  RowBox[{
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"theAlgebra", "=", 
        RowBox[{"First", "[", 
         RowBox[{"Cases", "[", 
          RowBox[{
           RowBox[{"First", "[", "vecOutProd", "]"}], ",", "_Cl", ",", 
           "Infinity"}], "]"}], "]"}]}], ",", "theBase", ",", "theVectors", 
       ",", "thePseudoscalar", ",", 
       RowBox[{"vecOutProdList", "=", 
        RowBox[{"List", "@@", 
         RowBox[{"(", "vecOutProd", ")"}]}]}]}], "}"}], ",", 
     RowBox[{
      RowBox[{"theBase", "=", 
       RowBox[{"gaDefineOrthonormalBasis", "[", "theAlgebra", "]"}]}], ";", 
      RowBox[{"theVectors", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"gaIndexUp", "[", 
          RowBox[{"DeleteCases", "[", 
           RowBox[{
            RowBox[{"gaGetMV", "[", 
             RowBox[{"theBase", ",", 
              RowBox[{"{", "1", "}"}]}], "]"}], ",", "0"}], "]"}], "]"}], 
         ")"}], "/.", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"-", "1"}], "\[Rule]", "1"}], "}"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"thePseudoscalar", "=", 
       RowBox[{"Last", "[", "theBase", "]"}]}], ";", 
      RowBox[{
       RowBox[{"Det", "[", 
        RowBox[{"Outer", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"gaPE", "[", 
            RowBox[{"InnerProduct", "[", "##", "]"}], "]"}], "&"}], ",", 
          "vecOutProdList", ",", "theVectors"}], "]"}], "]"}], "*", 
       "thePseudoscalar"}]}]}], "]"}], "/;", 
   RowBox[{
    RowBox[{
     RowBox[{"Union", "[", 
      RowBox[{"gaGetGrade", "/@", 
       RowBox[{"(", 
        RowBox[{"List", "@@", "vecOutProd"}], ")"}]}], "]"}], "===", 
     RowBox[{"{", 
      RowBox[{"{", "1", "}"}], "}"}]}], "&&", 
    RowBox[{
     RowBox[{"Length", "[", "vecOutProd", "]"}], "===", 
     RowBox[{"(", 
      RowBox[{"Plus", "@@", 
       RowBox[{"First", "[", 
        RowBox[{"Cases", "[", 
         RowBox[{"vecOutProd", ",", 
          RowBox[{"Cl", "[", 
           RowBox[{"p_", ",", "q_", ",", "___"}], "]"}], ",", "Infinity"}], 
