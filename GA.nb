            RowBox[{"allGrades", ",", "grade"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"Switch", "[", 
           RowBox[{"restGrades", ",", 
            RowBox[{"{", "}"}], ",", 
            RowBox[{
             RowBox[{"-", "1"}], "*", 
             RowBox[{"gaGetMV", "[", 
