     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"gaSort", "[", 
       RowBox[{"expr_List", ",", 
        RowBox[{"order", ":", 
         RowBox[{"(", 
          RowBox[{
          "\"\<Lex\>\"", "|", "\"\<InvLex\>\"", "|", "\"\<RevLex\>\"", "|", 
           "\"\<InvRevLex\>\"", "|", "\"\<Deg[Lex]\>\"", "|", 
           "\"\<Deg[InvLex]\>\"", "|", "\"\<Deg[RevLex]\>\"", "|", 
           "\"\<Deg[InvRevLex]\>\"", "|", "\"\<InvDeg[Lex]\>\"", "|", 
           "\"\<InvDeg[InvLex]\>\"", "|", "\"\<InvDeg[RevLex]\>\"", "|", 
