          RowBox[{"{", 
           RowBox[{"TextCell", "[", 
            RowBox[{"Column", "[", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"Row", "[", 
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{"TextCell", "[", "\"\<Blade \>\"", "]"}], ",", 
                  RowBox[{"ExpressionCell", "[", "fe", "]"}], ",", 
                  RowBox[{
                  "TextCell", "[", "\"\<commute with blades \>\"", "]"}], ",", 
                  RowBox[{"ExpressionCell", "[", "rez", "]"}]}], "}"}], "]"}],
                ",", 
               RowBox[{"Row", "[", 
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{
                  "TextCell", "[", "\"\<Next selected blade number is \>\"", 
